        "## 2.1 판매량 정리"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "lXaEf_JNmFKi"
      },
      "source": [
        "df = df.dropna()"
      ],
      "execution_count": 6,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "3yTffttanJft"
      },
      "source": [
        "def replace_KM(ser):\n",
        "  replace_K = lambda x: str(float(x.replace('K', '')) * 0.001) if x.find('K') != -1 else x\n",
        "  replace_M = lambda x: x.replace('M', '') if x.find('M') != -1 else x\n",
        "  to_float = lambda x: float(x) * 100\n",
        "\n",
        "  ser = ser.map(replace_K)\n",
        "  ser = ser.map(replace_M)\n",
        "  ser = ser.map(to_float)\n",
        "  return ser"
      ],
      "execution_count": 7,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "dRk0mt3IIjRQ"
      },
      "source": [
        "for col in range(5, 9):\n",
        "  df.iloc[:, col] = replace_KM(df.iloc[:, col])"
      ],
      "execution_count": 8,
      "outputs": []
    },
    {
      "cell_type": "markdown",
      "metadata": {
        "id": "AwEFZzIvUcrO"
      },
      "source": [
        "## 2.2 연도 정리"
      ]
    },
